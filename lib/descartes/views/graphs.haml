.well
  %a{ :class => "import btn", :"data-toggle" => "collapse", :"data-target" => "#import_form", :href => "#" } Add Graphs
  .columns.btn-group
    %button{ :class => "columns btn disabled" } Columns
    %button{ :class => "columns btn", :value => 1 } 1
    %button{ :class => "columns btn", :value => 2 } 2
    %button{ :class => "columns btn active", :value => 3 } 3
    %button{ :class => "columns btn", :value => 4 } 4
.accordion-group
  %fieldset#import_form{ :class => "collapse" }
    %form{ :method => "post", :action => "/graphs" }
      %ul
        %li
          %select{ :id => "node", :name => "node[]", :multiple => "multiple" }
        %li
          %button{ :class => "import_submit btn btn-primary", :name => "submit", :type => "submit" } Import Saved Graph
.graphs
%script{ :type => "text/javascript", :src => "/js/graph-import.js" }
%script{ :type => "text/javascript", :src => "/js/graph-render.js" }
